{
    "name": "lepre/routing",
    "description": "A unified interface to http routing, PSR-7 compliant",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele De Nobili",
            "email": "danieledenobili@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "zendframework/zend-diactoros": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Lepre\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lepre\\Routing\\Tests\\": "tests/"
        }
    },
    "provide": {
        "psr/container-implementation": "1.0"
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
