{
    "name": "lepre/routing",
    "description": "A unified interface to http routing, PSR-7 compliant",
    "homepage": "https://github.com/leprephp/routing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele De Nobili",
            "email": "danieledenobili@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0",
        "aura/router": "^3.1",
        "php-coveralls/php-coveralls": "^2.1",
        "zendframework/zend-diactoros": "^1.7"
    },
    "suggest": {
        "aura/router": "[\"^3.1\"] To use the Aura.Router adapter"
    },
    "autoload": {
        "psr-4": {
            "Lepre\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lepre\\Routing\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
